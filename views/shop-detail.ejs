    <script>
        function setPrice(priceMin, priceMax, discount) {
            let size = document.getElementById('pa_size').value;
            let dom = document.getElementById('final-price');
            let sizeArr = ['s ', 'm ', 'l ', 'xl ', 'xxl ', 'xxxl '];
            let steps = parseFloat(priceMax - priceMin) / parseFloat(sizeArr.length)
            let finalPrice = priceMin + parseFloat((sizeArr.indexOf(size) * steps).toFixed(0));
            if (size == 'xxxl ') {
                finalPrice += 1
            }
            dom.innerHTML = `<span class="old-price" style="font-weight: normal!important;font-size:1.9rem">$ ${finalPrice/discount}.99</span>`
            dom.innerHTML += '  $' + finalPrice + '.99';
        }
    </script>
